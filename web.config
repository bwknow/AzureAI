<configuration>
    <system.webServer>
       <directoryBrowse enabled="true" />
            <handlers>
                <!-- indicates that the app.js file is a node.js application to be handled by the iisnode module -->
                <add name="iisnode" path="./server/server.js" verb="*" modules="iisnode" />
            </handlers>
    </system.webServer>
</configuration>